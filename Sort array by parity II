class Solution {
    public int[] sortArrayByParityII(int[] nums) {
       int[] to_return = new int[nums.length];
        int even = 0;
        int odd = 1;
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]%2==0)
            {
                to_return[even] = nums[i];
                even+=2;
            }
            else
            {
                to_return[odd] = nums[i];
                odd+=2;
            }
        }
        return to_return;
    }
}
