int i=n-1,j=0;
      int temp=0;
      while(i>=0 && j<m)
      {
          if(arr1[i]>arr2[j])
          {
              temp=arr1[i];
              arr1[i]=arr2[j];
              arr2[j]=temp;
          }
          else
          break;
          i--;
          j++;
      }
      Arrays.sort(arr1);
      Arrays.sort(arr2);
